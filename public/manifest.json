  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "shortcuts": [
    {
      "name": "Search Artists",
      "short_name": "Search",
      "description": "Search for artists, shows, and venues",
      "url": "/search",
      "icons": [
        {
          "src": "/images/icon-search-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Trending Shows",
      "short_name": "Trending",
      "description": "View trending concerts and setlists",
      "url": "/trending",
      "icons": [
        {
          "src": "/images/icon-trending-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "My Artists",
      "short_name": "Following",
      "description": "View artists you're following",
      "url": "/following",
      "icons": [
        {
          "src": "/images/icon-following-96x96.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+mysetlist",
      "url": "/show/%s"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  }
