{
  "name": "mysetlist",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "start": "next start",
    "start:production": "NODE_ENV=production npm run start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "test": "echo \"Tests coming soon\" && exit 0",
    "test:performance": "node scripts/performance-test.js",
    "monitor:performance": "node scripts/performance-monitor.js",
    "monitor:enhanced": "node scripts/enhanced-performance-monitor.js",
    "optimize:database": "node scripts/database-optimization.js",
    "optimize:production": "node scripts/optimize-production.js",
    "validate:performance": "node scripts/validate-performance-optimizations.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "optimize": "npm run lint && npm run type-check && npm run build:analyze",
    "precommit": "npm run format && npm run lint:check && npm run type-check"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.4",
    "@supabase/auth-helpers-nextjs": "^0.7.2",
    "@supabase/auth-helpers-react": "^0.4.0",
    "@supabase/auth-ui-react": "^0.4.2",
    "@supabase/auth-ui-shared": "^0.1.6",
    "@supabase/supabase-js": "^2.26.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@vercel/analytics": "^1.0.1",
    "autoprefixer": "10.4.14",
    "date-fns": "^4.1.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "lucide-react": "^0.525.0",
    "next": "^14.2.28",
    "postcss": "^8.5.3",
    "prettier": "^3.0.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.9.0",
    "react-loader-spinner": "^6.1.6",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "3.3.2",
    "zod": "^3.24.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.0",
    "chrome-launcher": "^1.2.0",
    "lighthouse": "^12.7.1",
    "supabase": "^1.68.6",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
